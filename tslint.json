{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "tslint-sonarts"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "build/**",
      "styleguilde/**"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "jsx-self-close": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "jsx-no-bind": false,
    "jsx-boolean-value": false,
    "member-access": false
  }
}