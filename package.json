{
  "name": "first-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "start:prod": "cross-env REACT_APP_ENV=production npm start",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js styleguide:build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage --watchAll",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "typedoc": "typedoc --out docs --mode modules --tsconfig tsconfig.json",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "aws-amplify": "^1.1.18",
    "aws-sdk": "^2.382.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "react": "^16.4.0",
    "react-canvas-draw": "^0.1.9",
    "react-dom": "^16.4.0",
    "react-icons": "^3.2.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^4.0.8",
    "react-toastify": "^4.5.1",
    "reactstrap": "^6.5.0",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "rxjs": "^6.3.2",
    "tether": "^1.4.4"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-toastify": "^4.0.1",
    "@types/reactstrap": "^6.4.3",
    "@types/redux-logger": "^3.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "react-docgen-typescript": "^1.12.2",
    "react-styleguidist": "^8.0.6",
    "typedoc": "^0.12.0",
    "typescript": "^2.9.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
